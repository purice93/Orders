package com.view;

import java.awt.Color;
import java.awt.Graphics;

import javax.swing.JPanel;

public class Zq3d extends JPanel{
	@Override
	public void paint(Graphics g)
	{
		g.setColor(Color.lightGray);
		
		g.fill3DRect(0, 0, 60, 60, true);
		g.fill3DRect(0, 0, 60, 60, false);
		
		Color col=new Color(10,200,10);
		//定义一种笔col稍微暗一点的眼神
		Color darker=col.darker();
		g.fillRect(0, 80,80 , 83);
		g.setColor(col);
	}

}
