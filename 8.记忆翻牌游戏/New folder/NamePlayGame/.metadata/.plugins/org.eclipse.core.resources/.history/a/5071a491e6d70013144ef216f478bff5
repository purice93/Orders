package com.control;

public class Get {
	private boolean zhuangtai;
	private int x;
	private int y;

	public boolean isZhuangtai() {
		return zhuangtai;
	}

	public void setZhuangtai(boolean zhuangtai) {
		this.zhuangtai = zhuangtai;
	}
	public int getX() {
		return x;
	}

	public void setX(int x) {
		this.x = x;
	}

	public int getY() {
		return y;
	}

	public void setY(int y) {
		this.y = y;
	}
}
